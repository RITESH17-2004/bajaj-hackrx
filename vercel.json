{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/hackrx/run",
      "dest": "/api/hackrx"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    }
  ],
  "env": {
    "MISTRAL_API_KEY": "@mistral_api_key"
  }
}